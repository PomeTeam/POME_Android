<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            android:background="@color/white">

        <ImageView
            android:id="@+id/mypage_withdraw_warning_arrow_iv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_arrow_left_24"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="16dp"
            android:layout_marginLeft="12dp"
            android:layout_marginBottom="16dp"/>

        <TextView
            android:id="@+id/mypage_withdraw_name_tv"
            android:text="@string/mypage_setting_withdraw_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/Pome.Medium.14"
            android:textColor="@color/grey_8"
            app:layout_constraintTop_toTopOf="@+id/mypage_withdraw_warning_arrow_iv"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="@+id/mypage_withdraw_warning_arrow_iv"
            />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/mypage_withdraw_warning_tv"
            android:text="@string/mypage_withdraw_warning_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/Pome.SemiBold.20"
            android:textColor="@color/grey_8"
            app:layout_constraintTop_toBottomOf="@+id/cl"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginStart="20dp"
            android:layout_marginTop="10dp"
            />

        <TextView
            android:id="@+id/mypage_withdraw_warning_1_point_tv"
            android:text="@string/mypage_withdraw_warning_point_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/Pome.SemiBold.20"
            android:textColor="@color/grey_8"
            app:layout_constraintTop_toBottomOf="@+id/mypage_withdraw_warning_tv"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            />

        <TextView
            android:id="@+id/mypage_withdraw_warning_1_tv"
            android:text="@string/mypage_withdraw_warning_1_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/Pome.Regular.16"
            android:textColor="@color/grey_7"
            app:layout_constraintBottom_toBottomOf="@id/mypage_withdraw_warning_1_point_tv"
            app:layout_constraintLeft_toRightOf="@+id/mypage_withdraw_warning_1_point_tv"
            android:layout_marginStart="2dp"
            />

        <TextView
            android:id="@+id/mypage_withdraw_warning_2_point_tv"
            android:text="@string/mypage_withdraw_warning_point_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/Pome.SemiBold.20"
            android:textColor="@color/grey_8"
            app:layout_constraintTop_toBottomOf="@+id/mypage_withdraw_warning_1_point_tv"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginStart="20dp"
            android:layout_marginTop="10dp"
            />

        <TextView
            android:id="@+id/mypage_withdraw_warning_2_tv"
            android:text="@string/mypage_withdraw_warning_2_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/Pome.Regular.16"
            android:textColor="@color/grey_7"
            app:layout_constraintBottom_toBottomOf="@id/mypage_withdraw_warning_2_point_tv"
            app:layout_constraintLeft_toRightOf="@+id/mypage_withdraw_warning_2_point_tv"
            android:layout_marginStart="2dp"
            />

        <TextView
            android:id="@+id/mypage_withdraw_warning_3_point_tv"
            android:text="@string/mypage_withdraw_warning_point_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/Pome.SemiBold.20"
            android:textColor="@color/grey_8"
            app:layout_constraintTop_toBottomOf="@+id/mypage_withdraw_warning_2_point_tv"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginStart="20dp"
            android:layout_marginTop="10dp"
            />

        <TextView
            android:id="@+id/mypage_withdraw_warning_3_tv"
            android:text="@string/mypage_withdraw_warning_3_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/Pome.Regular.16"
            android:textColor="@color/grey_7"
            app:layout_constraintBottom_toBottomOf="@id/mypage_withdraw_warning_3_point_tv"
            app:layout_constraintLeft_toRightOf="@+id/mypage_withdraw_warning_3_point_tv"
            android:layout_marginStart="2dp"
            />

        <TextView
            android:id="@+id/mypage_withdraw_warning_4_point_tv"
            android:text="@string/mypage_withdraw_warning_point_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/Pome.SemiBold.20"
            android:textColor="@color/grey_8"
            app:layout_constraintTop_toBottomOf="@+id/mypage_withdraw_warning_3_point_tv"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginStart="20dp"
            android:layout_marginTop="10dp"
            />

        <TextView
            android:id="@+id/mypage_withdraw_warning_4_tv"
            android:text="@string/mypage_withdraw_warning_4_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxLines="2"
            style="@style/Pome.Regular.16"
            android:textColor="@color/grey_7"
            app:layout_constraintTop_toTopOf="@+id/mypage_withdraw_warning_4_point_tv"
            app:layout_constraintLeft_toRightOf="@+id/mypage_withdraw_warning_4_point_tv"
            android:layout_marginStart="2dp"
            android:layout_marginTop="5dp"
            android:paddingEnd="40dp"
            />


        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/withdraw_okay_btn"
            style="@style/Pome.Bold.18"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_marginHorizontal="16dp"
            android:layout_marginBottom="14dp"
            android:background="@drawable/register_profile_name_check_available_btn_background"
            android:stateListAnimator="@null"
            android:text="탈퇴하기"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />



    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>