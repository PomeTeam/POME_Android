<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/pome_dialog_cl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/pome_title_tv"
            style="@style/Pome.SemiBold.16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="로그아웃 하시겠어요?"
            android:textColor="@color/grey_9"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

        <TextView
            style="@style/Pome.Medium.14"
            android:id="@+id/pome_yes_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="네"
            android:textColor="@color/main"
            android:layout_marginTop="28dp"
            android:layout_marginEnd="24dp"
            android:paddingBottom="24dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/pome_title_tv"/>

        <TextView
            style="@style/Pome.Medium.14"
            android:id="@+id/pome_no_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="아니요"
            android:textColor="@color/main"
            android:layout_marginEnd="24dp"
            app:layout_constraintTop_toTopOf="@id/pome_yes_tv"
            app:layout_constraintEnd_toStartOf="@id/pome_yes_tv"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>