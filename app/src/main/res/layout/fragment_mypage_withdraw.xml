<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            android:background="@color/white">

        <ImageView
            android:id="@+id/mypage_withdraw_arrow_iv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_arrow_left_24"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="16dp"
            android:layout_marginLeft="12dp"
            android:layout_marginBottom="16dp"/>

        <TextView
            android:id="@+id/mypage_withdraw_name_tv"
            android:text="@string/mypage_setting_withdraw_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/Pome.Medium.14"
            android:textColor="@color/grey_8"
            app:layout_constraintTop_toTopOf="@+id/mypage_withdraw_arrow_iv"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="@+id/mypage_withdraw_arrow_iv"
            />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/mypage_withdraw_reason_tv"
            android:text="@string/mypage_withdraw_reason_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/Pome.SemiBold.20"
            android:textColor="@color/grey_8"
            app:layout_constraintTop_toBottomOf="@+id/cl"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginStart="20dp"
            android:layout_marginTop="10dp"
            />

        <TextView
            android:id="@+id/mypage_withdraw_know_tv"
            android:text="@string/mypage_withdraw_know_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/Pome.Medium.14"
            android:textColor="@color/grey_5"
            app:layout_constraintTop_toBottomOf="@+id/mypage_withdraw_reason_tv"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginStart="20dp"
            android:layout_marginTop="12dp"
            />

        <com.google.android.material.card.MaterialCardView
            android:id="@+id/mypage_withdraw_reason_1_cv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardBackgroundColor="@color/grey_0"
            app:cardCornerRadius="6dp"
            android:layout_marginHorizontal="20dp"
            app:layout_constraintTop_toBottomOf="@+id/mypage_withdraw_know_tv"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="30dp"
            >

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cv_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/withdraw_reason_1_aiv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="31dp"
                    android:src="@drawable/ic_check_24"
                    android:paddingStart="14dp"
                    android:paddingVertical="15dp"
                    app:layout_constraintBottom_toBottomOf="@id/cv_1"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="@id/cv_1" />

                <TextView
                    android:id="@+id/withdraw_reason_1_tv"
                    android:text="@string/mypage_withdraw_reason_1_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    style="@style/Pome.Regular.16"
                    android:textColor="@color/grey_8"
                    app:layout_constraintTop_toTopOf="@+id/withdraw_reason_1_aiv"
                    app:layout_constraintBottom_toBottomOf="@+id/withdraw_reason_1_aiv"
                    app:layout_constraintLeft_toRightOf="@id/withdraw_reason_1_aiv"
                    android:layout_marginStart="6dp"
                    />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView
            android:id="@+id/mypage_withdraw_reason_2_cv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardBackgroundColor="@color/grey_0"
            app:cardCornerRadius="6dp"
            android:layout_marginHorizontal="20dp"
            app:layout_constraintTop_toBottomOf="@+id/mypage_withdraw_reason_1_cv"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="12dp"
            >

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cv_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/withdraw_reason_2_aiv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="31dp"
                    android:src="@drawable/ic_check_24"
                    android:paddingStart="14dp"
                    android:paddingVertical="15dp"
                    app:layout_constraintBottom_toBottomOf="@id/cv_2"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="@id/cv_2" />

                <TextView
                    android:id="@+id/withdraw_reason_2_tv"
                    android:text="@string/mypage_withdraw_reason_2_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    style="@style/Pome.Regular.16"
                    android:textColor="@color/grey_8"
                    app:layout_constraintTop_toTopOf="@+id/withdraw_reason_2_aiv"
                    app:layout_constraintBottom_toBottomOf="@+id/withdraw_reason_2_aiv"
                    app:layout_constraintLeft_toRightOf="@id/withdraw_reason_2_aiv"
                    android:layout_marginStart="6dp"
                    />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView
            android:id="@+id/mypage_withdraw_reason_3_cv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardBackgroundColor="@color/grey_0"
            app:cardCornerRadius="6dp"
            android:layout_marginHorizontal="20dp"
            app:layout_constraintTop_toBottomOf="@+id/mypage_withdraw_reason_2_cv"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="12dp"
            >

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cv_3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/withdraw_reason_3_aiv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="31dp"
                    android:src="@drawable/ic_check_24"
                    android:paddingStart="14dp"
                    android:paddingVertical="15dp"
                    app:layout_constraintBottom_toBottomOf="@id/cv_3"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="@id/cv_3" />

                <TextView
                    android:id="@+id/withdraw_reason_3_tv"
                    android:text="@string/mypage_withdraw_reason_3_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    style="@style/Pome.Regular.16"
                    android:textColor="@color/grey_8"
                    app:layout_constraintTop_toTopOf="@+id/withdraw_reason_3_aiv"
                    app:layout_constraintBottom_toBottomOf="@+id/withdraw_reason_3_aiv"
                    app:layout_constraintLeft_toRightOf="@id/withdraw_reason_3_aiv"
                    android:layout_marginStart="6dp"
                    />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView
            android:id="@+id/mypage_withdraw_reason_4_cv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardBackgroundColor="@color/grey_0"
            app:cardCornerRadius="6dp"
            android:layout_marginHorizontal="20dp"
            app:layout_constraintTop_toBottomOf="@+id/mypage_withdraw_reason_3_cv"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="12dp"
            >

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cv_4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/withdraw_reason_4_aiv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="31dp"
                    android:src="@drawable/ic_check_24"
                    android:paddingStart="14dp"
                    android:paddingVertical="15dp"
                    app:layout_constraintBottom_toBottomOf="@id/cv_4"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="@id/cv_4" />

                <TextView
                    android:id="@+id/withdraw_reason_4_tv"
                    android:text="@string/mypage_withdraw_reason_4_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    style="@style/Pome.Regular.16"
                    android:textColor="@color/grey_8"
                    app:layout_constraintTop_toTopOf="@+id/withdraw_reason_4_aiv"
                    app:layout_constraintBottom_toBottomOf="@+id/withdraw_reason_4_aiv"
                    app:layout_constraintLeft_toRightOf="@id/withdraw_reason_4_aiv"
                    android:layout_marginStart="6dp"
                    />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </com.google.android.material.card.MaterialCardView>

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/withdraw_okay_btn"
            style="@style/Pome.Bold.18"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_marginHorizontal="16dp"
            android:layout_marginBottom="14dp"
            android:background="@drawable/register_profile_name_check_disable_btn_background"
            android:enabled="false"
            android:stateListAnimator="@null"
            android:text="확인"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />



    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>